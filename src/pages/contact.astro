---
import Layout from "../layouts/Start3Layout.astro";
import Header from "../components/start3/Header.astro";
import CtaSection from "../components/start3/CtaSection.astro";
import Footer from "../components/start3/Footer.astro";
---

<Layout
  title="Start3 - Votre présence web en 3 jours pour 800€"
  canonical="https://start3.tech-talent.fr/contact"
>
  <div class="page-wrapper">
    <Header />
    <div class="header-margin"></div>
    <CtaSection />
    <Footer />
  </div>
</Layout>

<style>
  .page-wrapper {
    display: flex;
    flex-direction: column;
    min-height: 100vh;
  }

  .header-margin {
    height: 140px;
  }
</style>
